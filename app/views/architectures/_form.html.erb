<%= form_for @architecture, :url => (@architecture.new_record? ? architectures_path : architecture_path(:id => @architecture.id)) do |f| %>
  <%= base_errors_for @architecture %>
  <%= text_f f, :name %>
  <%= mount_dual_list(:operatingsystems, @architecture, Operatingsystem) %>
  <%= submit_or_cancel f %>
<% end %>
